#! /bin/bash

CFL_AA="-use-cfl-aa-in-codegen=both"
LD_FLAGS=" --allow-undefined-file=${CMAKE_INSTALL_PREFIX}/eosio.imports -e apply --lto-O3 --gc-sections --merge-data-segments --strip-all -stack-first -zstack-size=8192 -mllvm $CFL_AA -lc++ -lc -leosio "

${CMAKE_INSTALL_PREFIX}/bin/wasm-ld $@ -L${CMAKE_INSTALL_PREFIX}/lib $LD_FLAGS
